{
    "devCommand": "uvicorn app.main:app --host 0.0.0.0 --port 8000",
    "builds": [
        {
            "src": "app/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "methods": [
                "OPTIONS"
            ],
            "status": 204,
            "continue": false
        },
        {
            "src": "/(.*)",
            "dest": "app/main.py"
        }
    ]
}